  "description": "Mini sistema KPI para vendedores con Node.js + PostgreSQL.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "migrate": "npx sequelize-cli db:migrate --config ./config/config.json --migrations-path ./migrations",
    "migrate:undo": "npx sequelize-cli db:migrate:undo --config ./config/config.json --migrations-path ./migrations",
    "dev": "nodemon server.js",
    "start": "node server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
